<%- include("partials/header") -%>


<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
    <label>Title</label>
    <input type="text" name="postTitle" class="form-control" placeholder="Type in your text">
    <label>Post</label>
    <textarea name="postBody" class="form-control" cols="50" rows="5"></textarea>
</div>
<button class="btn btn-primary" type="submit">Publish</button>
</form>

